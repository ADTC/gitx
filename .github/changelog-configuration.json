{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "feature",
        "enhancement"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix",
        "bug"
      ]
    },
    {
      "title": "## 💬 Maintenance",
      "labels": [
        "maintenance"
      ]
    }
  ],
  "ignore_labels": [
    "dependencies",
    "gradle-wrapper"
  ],
  "sort": "ASC",
  "tag_resolver": {
    "filter": {
      "pattern": "^\\d"
    }
  },
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "empty_template": "- no changes"
}
